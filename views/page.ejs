<!-- Blurred image -->
<script src="https://unpkg.com/fast-average-color/dist/index.min.js"></script>
<script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>

<script>	


	// My Color
	const needBackground = 
	[{
		id: "me", 
		value: "<%- me.logo_url %>"
	},
	{
		id: "them",
		value: "<%- person.org.logo_url %>"
	}];
	needBackground.forEach(setBackground);


	var bgSet = needBackground.length;
	function setBackground(elem) {
		console.log(elem);
		$("#logo_" + elem.id).one('load', function() {
			colorCover(elem.id);

			bgSet -= 1;
			if (bgSet == 0) {
				animateIn();
			}
		}).on('error', function(e) {
			colorCover();
		}).attr('src', elem.value);
	}



	function colorCover(personId) {
		const fac = new FastAverageColor();
		const container = document.getElementById("container_logo_" + personId);

		const color = fac.getColor(document.getElementById("logo_" + personId), {algorithm: "simple"});
		container.style.backgroundColor = color.hex + "1A";
		// container.style.color = color.isDark ? '#fff' : '#000';		
		console.log(color);
	}

	function animateIn() {
		var replacers = document.querySelectorAll('[data-replace]');
		for(var i=0; i<replacers.length; i++){
			let replaceClasses = JSON.parse(replacers[i].dataset.replace.replace(/'/g, '"'));
			Object.keys(replaceClasses).forEach(function(key) {
				replacers[i].classList.remove(key);
				replacers[i].classList.add(replaceClasses[key]);
			});
		}
	}

</script>


