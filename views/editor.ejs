<body class="bg-slate-100">

	<!-- Left pane -->
	<section class="bg-red-500 fixed top-0 bottom-0 left-0 w-60 bg-slate-800 py-4 px-2">
		<div class="p-3 rounded-lg flex items-center text-slate-400 bg-slate-900 cursor-pointer">
			<svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
				<path stroke-linecap="round" stroke-linejoin="round" d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10" />
			</svg>
			<span class="text-slate-300 ml-3">Templates</span>
		</div>
	</section>


	<!-- Content -->
	<section class="ml-60 px-8 pb-16">
		<section class="max-w-7xl mx-auto">

			<!-- Title -->
			<div class="my-10 flex items-center justify-between">
				<div>
					<input type="text" class="text-slate-800 text-3xl font-semibold border-b-2 border-transparent hover:border-indigo-500 bg-transparent focus:outline-none inline-block transition" value="engineers">
					<p class="text-slate-400 font-light mt text-sm">https://paage.io/neil/engineers</p>
				</div>
				<div class="flex items-center space-x-4">
					<button class="rounded-md bg-indigo-500 py-2 px-5 text-white hover:bg-indigo-600 transition border border-transparent" tabindex="-1">Save</button>
				</div>
			</div>

			<!-- Body -->
			<div class="grid grid-cols-3 mx-auto gap-7">

				<!-- Form -->
				<div class="col-span-2 ">

					<!-- Form content -->
					<div class="bg-white space-y-7 px-4 py-5 sm:p-7 rounded-lg flex-1 shadow text-slate-800 text-sm">

						<!-- Navigation -->
						<div class="">

							<!-- Section Input -->
							<div class="md:col-span-2 grid grid-cols-2 gap-6">

								<!-- Page title -->
								<div class="col-span-1">
									<label for="page-title" class="block font text-slate-700">Page Title</label>
									<div class="mt-1.5">
										<input type="text" name="page-title" id="page-title" class="border border-slate-200 rounded-md shadow-sm py-2 px-3 transition w-full" value="{{company}} + {{me}}" placeholder="{{company}} + {{me}}">
									</div>
								</div>

								<!-- Main CTA -->
								<div class="col-span-1">
									<label for="page-title" class="block font text-slate-700">Calendar Button</label>
									<div class="mt-1.5">
										<input type="text" name="page-title" id="page-title" class="border border-slate-200 rounded-md shadow-sm py-2 px-3 transition w-full" placeholder="Book a Meeting" value="Book a Meeting">
									</div>
								</div>
							</div>
						</div>


						<!-- Video link -->
						<div class="grid grid-cols-3 gap-6">

							<!-- Video URL -->
							<div class="col-span-3 sm:col-span-2">
								<label for="page-title" class="block font-medium text-slate-700">Video URL </label>
								<p class="text-slate-400">Loom, Vidyard, Bombbomb, or Youtube link</p>
								<div class="mt-1.5">
									<input type="text" name="page-title" id="page-title" class="border border-slate-200 rounded-md shadow-sm py-2 px-3 transition w-full" placeholder="https://loom.com/share/abc">
								</div>
							</div>
						</div>


						<!-- Additional text -->
						<div class="">

							<!-- More info -->
							<div class="">
								<p class="block font-medium text-slate-700">More info</p>

								<!-- Blocks of info -->
								<div id="container_info" class="mt-1.5 space-y-2 hidden">

									<!-- Each block -->
									<!-- <div class="bg-slate-100 p-4 space-y-4 rounded-md border border-slate-200">

										<div>
											<label for="page-title" class="block font-medium text-sm text-slate-700">Heading</label>
											<input type="text" name="page-title" id="page-title" class="border border-slate-200 rounded-md shadow-sm py-2 px-3 transition w-full mt-1" placeholder="Here are some of our features">
										</div>

										<div>
											<label for="page-title" class="block font-medium text-sm text-slate-700">Content</label>
											<textarea class="w-full bg-white border border-slate-200 rounded-md shadow-sm py-2 px-3 transition mt-1"></textarea>
										</div>
									</div> -->
								</div>

								<!-- Add new block -->
								<div class="mt-2">
									<button id="add_info-block" type="button" class="bg-white py-2 px-3 border border-slate-200 rounded-md shadow-sm text-sm leading-4 font-medium text-slate-600 hover:bg-slate-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500">＋ Add</button>
								</div>
							</div>
						</div>


						<!-- Customer logos -->
						<div class="grid grid-cols-3 gap-6">

							<!-- Customer logos -->
							<div class="col-span-3 sm:col-span-2">
								<label for="page-title" class="block font-medium text-slate-700">Customers</label>
								<div class="mt-1.5 mb-2">
									<input type="text" name="page-title" id="page-title" class="border border-slate-200 rounded-md shadow-sm py-2 px-3 transition hidden" placeholder="Hooli">
								</div>

								<div>
									<button type="button" class="bg-white py-2 px-3 border border-slate-200 rounded-md shadow-sm text-sm leading-4 font-medium text-slate-600 hover:bg-slate-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500">＋ Add</button>
								</div>
							</div>
						</div>

						<!-- Testimonial -->
						<div>
							<!-- Quote -->
							<div class="col-span-3 sm:col-span-2">
								<label for="page-title" class="block font-medium text-slate-700">Testimonial</label>
								<div class="mt-1.5 bg-slate-100 p-4 space-y-4 rounded-md border border-slate-200 hidden">

									<div class="grid grid-cols-2 gap-6">
										<div class="col-span-1">
											<label for="page-title" class="block font-medium text-sm text-slate-700">Name</label>
											<input type="text" name="page-title" id="page-title" class="border border-slate-200 rounded-md shadow-sm py-2 px-3 transition w-full mt-1" placeholder="John Doe">
										</div>

										<div class="col-span-1">
											<label for="page-title" class="block font-medium text-sm text-slate-700">Title</label>
											<input type="text" name="page-title" id="page-title" class="border border-slate-200 rounded-md shadow-sm py-2 px-3 transition w-full mt-1" placeholder="CFO">
										</div>
									</div>

									<div class="">
										<label for="page-title" class="block font-medium text-sm text-slate-700">Profile Pic</label>
										<input type="text" name="page-title" id="page-title" class="border border-slate-200 rounded-md shadow-sm py-2 px-3 transition w-full mt-1" placeholder="https://">
									</div>

									<div class="">
										<label for="page-title" class="block font-medium text-sm text-slate-700">Quote</label>
										<textarea class="w-full bg-white border border-slate-200 rounded-md shadow-sm py-2 px-3 transition mt-1">
											
										</textarea>
									</div>
								</div>
								<div class="mt-2">
									<button type="button" class="bg-white py-2 px-3 border border-slate-200 rounded-md shadow-sm text-sm leading-4 font-medium text-slate-600 hover:bg-slate-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500">＋ Add</button>
								</div>
							</div>
						</div>


						<!-- Calendar Link -->
						<div class="grid grid-cols-3 gap-6">

							<!-- Calendar Link -->
							<div class="col-span-3 sm:col-span-2">
								<label for="page-title" class="block font-medium text-slate-700">Your calendar link </label>
								<p class="text-slate-400">Calendly or Chilipiper</p>
								<div class="mt-1.5">
									<input type="text" name="page-title" id="page-title" class="w-full border border-slate-200 rounded-md shadow-sm py-2 px-3 transition" placeholder="https://calendly.com/30min/xxx">
								</div>
							</div>
						</div>
					</div>	

					<!-- Disclores -->
					<!-- <div class="text-sm flex items-start mt-6 space-x-2">
						<div class="text-slate-400 inline-block">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
								<path stroke-linecap="round" stroke-linejoin="round" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
							</svg>
						</div>
						<span class="text-slate-400">Use {{company}} for company name and {{name}} for first name anywhere in your paage. We use leading data vendors to get their company logo for you.</span>
					</div> -->
				</div>
				



				<!-- Thumbnail -->
				<div class="text-sm text-slate-400">
						
					<!-- Edit thumbnail -->
					<div class="bg-white divide-y divide-slate-200 bg-white rounded-lg shadow inline-block w-full overflow-hidden">

						<!-- Template name -->
						<!-- <div class="grid grid-cols-5 px-5 py-4">
							<label class="col-span-2">Template name</label>
							<input type="text" name="" class="col-span-3 focus:outline-none bg-transparent text-slate-800" value="engineers">
						</div> -->

						<!-- Link title -->
						<div class="grid grid-cols-5 px-5 py-4">
							<label class="col-span-2">Link title</label>
							<input type="text" name="" class="col-span-3 focus:outline-none bg-transparent text-slate-800" value="{{company}} + {{me}}">
						</div>

						<!-- Link preview -->
						<div class="px-5 py-4 bg-white select-none">
							<div class="grid grid-cols-5 mb-4">
								<div class="col-span-2">Preview link to</div>
								<div class="col-span-3 flex items-center">
									<span class="text-slate-800">Select person</span>
									<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor">
										<path fill-rule="evenodd" d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z" clip-rule="evenodd" />
									</svg>
								</div>
							</div>

							<a href="https://google.com/" target="_blank">
								<div class="border border-slate-300 rounded-md overflow-hidden">
									<div style="background-image: url('https://d33w6qmiqoj74y.cloudfront.net/RlYZir00-ryan@heap.io-ryan.png')" class="h-44 bg-center bg-cover"></div>

									<div class="bg-slate-100 py-3 px-4 text-sm">
										<div class="font-semibold text-slate-900">
											More leads to Heap?
										</div>
										<div class="text-slate-500">
											https://paage.io/neil/engineers/
										</div>
									</div>
								</div>
							</a>
						</div>
					</div>


					<!-- Link button -->
					<button class="flex items-center justify-center rounded-md w-full text-center. bg-white border border-slate-200 shadow-sm py-2.5 px-5 text-indigo-500 hover:bg-slate-50 font-medium transition mt-2">
						<svg xmlns="http://www.w3.org/2000/svg" class="h-4 w-4" viewBox="0 0 20 20" fill="currentColor">
							<path fill-rule="evenodd" d="M12.586 4.586a2 2 0 112.828 2.828l-3 3a2 2 0 01-2.828 0 1 1 0 00-1.414 1.414 4 4 0 005.656 0l3-3a4 4 0 00-5.656-5.656l-1.5 1.5a1 1 0 101.414 1.414l1.5-1.5zm-5 5a2 2 0 012.828 0 1 1 0 101.414-1.414 4 4 0 00-5.656 0l-3 3a4 4 0 105.656 5.656l1.5-1.5a1 1 0 10-1.414-1.414l-1.5 1.5a2 2 0 11-2.828-2.828l3-3z" clip-rule="evenodd" />
						</svg>
						<span class="ml-2">Get Link</span>
					</button>

				</div>
			</div>
		</section>	
	</section>
</body>


<script type="text/javascript" src="/js/dist/editor.js"></script>



</html>